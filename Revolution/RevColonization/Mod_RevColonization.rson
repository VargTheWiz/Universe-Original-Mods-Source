        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  580,
          "Pos.y":  330,
          "Parent":  -1,
          "#":  0,
          "Total.Lines":  336,
          "Code.Type":  "Init",
          "Code":          
          [
              "//NewsAdd(ScriptRun(ShipStar(Player()),GetShipPlanet(Player()),'Mod_RevColonization'));",
              "//Значения переменной scrStatus: 0 - скрипт запущен, ничего нет;",
              "//1 - найдена подходящая система, добавлен диалог на МЦ;",
              "//2 - игрок оплатил постройку базы на незаселенной планете;",
              "//3 - база приземлилась на планету, запущен таймер до конца;",
              "//100 - завершение колонизации или же отказ от нее;",
              "//200 - заселение планеты;",
              "//Значения переменной rewardStatus: 0 - нет предмета, ведутся раскопки",
              "//1 - предмет найден",
              "//2 - доставляется к игроку транспортом",
              "//Функция выбора системы для колонизации. Ищет неоспариваемую коалиционную систему с МЦ.",
              "//Возвращает указатель на систему или 0.",
              "function SelectSystem() {",
              "    int i;",
              "    dword system, ruin;",
              "    for(i=5;i<GalaxyStars();i=i+1) {    //Пропуск родных систем кажой расы",
              "        system=GalaxyStar(i);",
              "        if(StarOwner(system)) continue;    //Пропуск некоалиционных систем",
              "        if(StarBattle(system)) continue;    //Пропуск оспариваемых систем",
              "        if(!StarRuins(system)) continue;    //Пропуск систем без станций",
              "        ruin=StarRuins(system,'MC');        //Поиск МЦ",
              "        if(ruin) ArrayAdd(tempArray,system);",
              "    }",
              "    if(ArrayDim(tempArray)<=1) system=0;",
              "    else system=tempArray[Rnd(1,ArrayDim(tempArray)-1)];",
              "    ArrayClear(tempArray);",
              "    result=system;",
              "}",
              "//Функция переноса кораблей из МЦ на планету.",
              "function TransferShipsToPlanet(dword ruins, dword planetId) {",
              "    int i;",
              "    dword ship, planet, star;",
              "    star=ShipStar(ruins);",
              "    planet=IdToPlanet(planetId);",
              "    for(i=0;i<StarShips(star);i=i+1) {",
              "        ship=StarShips(star,i);",
              "        if(ship==Player()) continue;",
              "        if(GetShipRuins(ship)==ruins) TransferShip(ship,planet);",
              "    }",
              "}",
              "//Функция поиска наибольшей цены на товары в системе. Первый параметр - Id системы,",
              "//Второй - тип товара соответствующий prodType. Возвращает либо цену конкретного товара",
              "function FindOutPrices(dword starId, dword productionType) {",
              "    int i;",
              "    dword star, planet, price, tempPrice, goodsType;",
              "    star=GalaxyStar(starId-1);",
              "    price=0;",
              "    if(productionType==1) goodsType=t_Minerals;",
              "    else if(productionType==2) goodsType=t_Food;",
              "    else if(productionType==3) goodsType=t_Medicine;",
              "    else if(productionType==4) goodsType=t_Alcohol;",
              "    else goodsType=t_Luxury;",
              "    for(i=0;i<StarPlanets(star);i=i+1) {",
              "        planet=StarPlanets(star,i);",
              "        tempPrice=GoodsBuyPrice(planet,goodsType);",
              "        if(tempPrice>price) price=tempPrice;",
              "    }",
              "    result=price;",
              "}",
              "//Функция инициализации массивов наград и событий",
              "function InitArrays() {",
              "    int i, itemTL, itemSize, itemOwner;",
              "    dword tempItem, itemSubtype;",
              "    str itemTypeStr, subTypeStr, copyStr;",
              "    eventsCount=15;",
              "    for(i=0;i<eventsCount;i=i+1) ArrayAdd(eventsArray,0);",
              "    rewardsCount=CT('ColonizationRewards.TotalRewards');",
              "    for(i=0;i<rewardsCount;i=i+1) ArrayAdd(rewardsArray,0);",
              "    for(i=1;i<ArrayDim(rewardsArray);i=i+1) {",
              "        itemTypeStr=CT('ColonizationRewards.'+i+'.Type');",
              "        if(itemTypeStr=='useless') {",
              "            subTypeStr=CT('ColonizationRewards.'+i+'.Subtype');",
              "            copyStr=CT('ColonizationRewards.'+i+'.Owner');",
              "            itemOwner=GetValueFromScript('',copyStr);",
              "            tempItem=CreateQuestItem(subTypeStr,itemOwner);",
              "        }",
              "        else if(itemTypeStr=='artifact') {",
              "            itemSubtype=GetValueFromScript('',CT('ColonizationRewards.'+i+'.Subtype'));",
              "            copyStr=CT('ColonizationRewards.'+i+'.Owner');",
              "            if(copyStr=='-1') itemOwner=Rnd(Maloc,Gaal);",
              "            else itemOwner=GetValueFromScript('',copyStr);",
              "            tempItem=CreateArt(itemSubtype,itemOwner);",
              "        }",
              "        else if(itemTypeStr=='equipment') {",
              "            itemSubtype=GetValueFromScript('',CT('ColonizationRewards.'+i+'.Subtype'));",
              "            copyStr=CT('ColonizationRewards.'+i+'.Owner');",
              "            if(copyStr=='-1') itemOwner=Rnd(Maloc,Gaal);",
              "            else itemOwner=GetValueFromScript('',copyStr);",
              "            itemSize=CT('ColonizationRewards.'+i+'.Size');",
              "            if(itemSize<0) {",
              "                if(itemSubtype==t_FuelTanks) itemSize=CT('Items.FuelTanks.AverageSize');",
              "                else if(itemSubtype==t_Engine) itemSize=CT('Items.Engine.AverageSize');",
              "                else if(itemSubtype==t_Radar) itemSize=CT('Items.Radar.AverageSize');",
              "                else if(itemSubtype==t_Scaner) itemSize=CT('Items.Scaner.AverageSize');",
              "                else if(itemSubtype==t_RepairRobot) itemSize=CT('Items.RepairRobot.AverageSize');",
              "                else if(itemSubtype==t_CargoHook) itemSize=CT('Items.CargoHook.AverageSize');",
              "                else if(itemSubtype==t_DefGenerator) itemSize=CT('Items.DefGenerator.AverageSize');",
              "                else if(itemSubtype>t_DefGenerator && itemSubtype<t_CustomWeapon) itemSize=CT('Items.Weapon.Stats.'+(itemSubtype-t_DefGenerator)+'.AverageSize');",
              "                itemSize=round(Rnd(itemSize/2,itemSize*2));",
              "            }",
              "            itemTL=CT('ColonizationRewards.'+i+'.ItemTechLevel');",
              "            if(itemTL<0) itemTL=GalaxyTechLevel();",
              "            tempItem=CreateEquipment(itemSubtype,itemSize,itemTL,itemOwner);",
              "        }",
              "        PutItemInVault('RevColonization_Vault_'+i,tempItem);",
              "    }",
              "}",
              "//Функция создания корабля-доставщика",
              "function BuyDelivery(dword planetId) {",
              "    dword planet, ship;",
              "    planet=IdToPlanet(planetId);",
              "    ship=BuyTransport(planet);",
              "    ShipJoin(DeliveryGroup,ship);",
              "    TransferShip(ship, planet);",
              "    NoTargetToShip(ship,3);",
              "}",
              "//Функция генерации случайных событий для колонии. Все доступные события заносятся во временный массив",
              "//После чего одно из них случайно выбирается и срабатывает.",
              "//Отношения не могут опуститься до враждебных по событию.",
              "function CreateEvent() {",
              "    int i;",
              "    dword curEvent, curRelations;",
              "    str newsText, addition;",
              "    for(i=1;i<ArrayDim(eventsArray);i=i+1) {",
              "        if(!eventsArray[i]) ArrayAdd(tempArray,i);",
              "    }",
              "    addition='';",
              "    if(ArrayDim(tempArray)<=1) curEvent=0;",
              "    else {",
              "        curEvent=tempArray[Rnd(1,ArrayDim(tempArray)-1)];",
              "        if(curEvent==1) {",
              "            newsText=\"Сегодня на планете <Name(IdToPlanet(planetColonyId))> был ликвидирован последний очаг заражения гранэкской чумой, завезенной туда малокской экспедицией. Чудесное спасение сотен мензольских жизней стало возможно благодаря усилиям медицинского персонала планетарной базы, построенной на средства рейнджера <ShortName(Player())>.\";",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,20,2+4+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,20,4+8+16+32,1+2+4+8+16);",
              "        }",
              "        else if(curEvent==2) {",
              "            newsText=\"Торговая экспансия мензольской расы продолжается! Благодаря усилиям исследователей планетарной базы на планете <Name(IdToPlanet(planetColonyId))> состоялась сделка между представителями Коалиции и мензольской расы. Напомним, что исследования мензольской культуры были забыты еще в ХХХ веке, мосле смерти главного исследователя мензолов - Рене Маккалистера. И вот теперь, спустя триста лет, исследования возобновлены. Это стало возможно благодаря рейнджеру <ShortName(Player())>, меценату, профинансировавшему постройку базы на <Name(IdToPlanet(planetColonyId))>\";",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+2+4+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+2+4+8+16);",
              "        }",
              "        else if(curEvent==3) {",
              "            newsText=\"Силами мензольской службой безопасности был найден и обезврежен пеленгский шпионский зонд на планете <Name(IdToPlanet(planetColonyId))>. Пеленгское правительство пока воздержалось от комментариев. Напомним, что на этой планете располагается планетарная станция, призванная помочь в развитии мензольской расе и изучить древнюю культуру этого народа. По одной из гипотез, мензолы в древности контактировали с представителями других рас, явно не входящих в список известных. Если данная теория верна, перед исследователями открываются огромные перспективы как в культурно-историческом плане, так и в военно-техническом.\";",
              "            curRelations=GlobalRelationsShips(Player(),0,4+8+16+32,2);",
              "            if(curRelations<30) ChangeGlobalRelationsShips(Player(),0,0,10,4+8+16+32,2);",
              "            else ChangeGlobalRelationsShips(Player(),0,3,20,4+8+16+32,2);",
              "            curRelations=GlobalRelationsPlanets(Player(),0,2);",
              "            if(curRelations<30) ChangeGlobalRelationsPlanets(Player(),0,0,10,2);",
              "            else ChangeGlobalRelationsPlanets(Player(),0,3,20,2);",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+4+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+4+8+16);",
              "        }",
              "        else if(curEvent==4) {",
              "            newsText=\"Мензолы - последняя надежда Галактики. Экспедиция на планете <Name(IdToPlanet(planetColonyId))> при сотрудничестве с мензольскими мыслителями и учеными разработала новый алгоритм для экспериментальной антидоминаторской программы. Революционность разработки проявляется в том, что данная программа передает вполне осмысленные команды бортовому компьютеру доминаторского корабля, который принимает их за подлинные данные и пытается обработать. Мензольские специалисты использовали в качестве логической модели данной программы модель поведения своего народа, которая кардинально отличается от раннее использовавшегося подхода к решению задач подобного типа как у ученых рас Коалиции, так и у самих доминаторов. Однако, многие скептически отнеслись к подобной разработке. Больше всего сомнений в эффективности новой разработки высказали фэянские ученые. Их главный аргумент заключается в том, что мензольская раса еще слишком молода и обладает очень приблизительными знаниями об устройстве логических аппаратов доминаторов, потому программа будет бесполезна в борьбе с кремниевым разумом.\";",
              "            curRelations=GlobalRelationsShips(Player(),0,4+8+16+32,8);",
              "            if(curRelations<30) ChangeGlobalRelationsShips(Player(),0,0,10,4+8+16+32,8);",
              "            else ChangeGlobalRelationsShips(Player(),0,3,20,4+8+16+32,8);",
              "            curRelations=GlobalRelationsPlanets(Player(),0,8);",
              "            if(curRelations<30) ChangeGlobalRelationsPlanets(Player(),0,0,10,8);",
              "            else ChangeGlobalRelationsPlanets(Player(),0,3,20,8);",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+2+4+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+2+4+16);",
              "        }",
              "        else if(curEvent==5) {",
              "            newsText=\"Малокские власти подали жалобу в Галактический Совет на рейнджера <ShortName(Player())>. Известный рейнджер <ShortName(Player())>, профинансировавший постройку планетарного медицинского центра на планете <Name(IdToPlanet(planetColonyId))> может быть осужден. Малокские представители утверждают, что уже начали колонизировать <Name(IdToPlanet(planetColonyId))>, однако команда рейнджера <ShortName(Player())> выбросила малокских колонизаторов в открытый космос, чем и прервала на некоторое время экспансию великой малокской державы. Ведется следствие.\";",
              "            curRelations=GlobalRelationsShips(Player(),0,4+8+16+32,1);",
              "            if(curRelations<30) ChangeGlobalRelationsShips(Player(),0,0,10,4+8+16+32,1);",
              "            else ChangeGlobalRelationsShips(Player(),0,3,20,4+8+16+32,1);",
              "            curRelations=GlobalRelationsPlanets(Player(),0,1);",
              "            if(curRelations<30) ChangeGlobalRelationsPlanets(Player(),0,0,10,1);",
              "            else ChangeGlobalRelationsPlanets(Player(),0,3,20,1);",
              "        }",
              "        else if(curEvent==6) {",
              "            newsText=\"Сегодня на колонизационной базе планеты <Name(IdToPlanet(planetColonyId))> произошла трагедия: был взорван четвертый энергоблок станции. Есть погибшие как со стороны персонала станции, так и потери среди мензолов. Данное событие значительно ухудшило отношения между аборигенами и представителями Коалиции. Пеленгские специалисты утверждают, что данную диверсионную операцию провели малокские спецслужбы, однако гаальские и фэянские эксперты придерживаются мнения, что имела место халатность персонала.\";",
              "            curRelations=GlobalRelationsShips(Player(),0,4+8+16+32,8+16);",
              "            if(curRelations<20) ChangeGlobalRelationsShips(Player(),0,0,10,4+8+16+32,8+16);",
              "            else ChangeGlobalRelationsShips(Player(),0,3,10,4+8+16+32,8+16);",
              "            curRelations=GlobalRelationsPlanets(Player(),0,8+16);",
              "            if(curRelations<20) ChangeGlobalRelationsPlanets(Player(),0,0,10,8+16);",
              "            else ChangeGlobalRelationsPlanets(Player(),0,3,10,8+16);",
              "        }",
              "        else if(curEvent==7) {",
              "            newsText=\"Гаальское правительство отзывает своих специалистов с колонизационной базы на планете <Name(IdToPlanet(planetColonyId))>. Официальная причина - пагубное влияние исследователей на уникальную мензольскую культуру. В противовес этому решению, малокская и пеленгская делегации поступили с точностью до наоборот, отправив дополнительных специалистов на планету <Name(IdToPlanet(planetColonyId))>. Это решение мотивировано тем, что уменьшение численности персонала станции может поставить под угрозу весь исследовательский проект.\";",
              "            curRelations=GlobalRelationsShips(Player(),0,4+8+16+32,16);",
              "            if(curRelations<20) ChangeGlobalRelationsShips(Player(),0,0,10,4+8+16+32,16);",
              "            else ChangeGlobalRelationsShips(Player(),0,3,10,4+8+16+32,16);",
              "            curRelations=GlobalRelationsPlanets(Player(),0,8+16);",
              "            if(curRelations<20) ChangeGlobalRelationsPlanets(Player(),0,0,10,16);",
              "            else ChangeGlobalRelationsPlanets(Player(),0,3,10,16);",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+2);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+2);",
              "        }",
              "        else if(curEvent==8) {",
              "            newsText=\"На планете <Name(IdToPlanet(planetColonyId))> мензольскими силами при содействии планетарной базы налажена добыча редких минеральных солей. Вне всякого сомнения, этот товар найдет своего покупателя на многих планетах Коалиции. Однако, человеческое правительство не оценило такой рывок в мензольской промышленности, так как поставкой руд и минералов традиционно занимались торговые компании под руководством людей.\";",
              "            curRelations=GlobalRelationsShips(Player(),0,4+8+16+32,4);",
              "            if(curRelations<30) ChangeGlobalRelationsShips(Player(),0,0,10,4+8+16+32,4);",
              "            else ChangeGlobalRelationsShips(Player(),0,3,20,4+8+16+32,4);",
              "            curRelations=GlobalRelationsPlanets(Player(),0,4);",
              "            if(curRelations<30) ChangeGlobalRelationsPlanets(Player(),0,0,10,4);",
              "            else ChangeGlobalRelationsPlanets(Player(),0,3,20,4);",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+2+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+2+8+16);",
              "            if(prodType==1) {",
              "                prodOutput=prodOutput+4;    //Бонус к производству минералов",
              "                addition=\"Сообщение с планеты <Name(IdToPlanet(planetColonyId))>.<br><Player> вы следите за последними новостями? Так вот, уведомляем вас о том, что последние события благотворно сказались на производстве <prodStr>.\";",
              "            }",
              "       }",
              "        else if(curEvent==9) {",
              "            newsText=\"Срочная новость! На планете <Name(IdToPlanet(planetColonyId))> начались потасовки среди представителей мензольской расы. Причины беспорядков до сих пор не выяснены, однако ситуацию удалось стабилизировать персоналу планетарной базы, руководителем которой является рейнджер <ShortName(Player())>. Инцидент обошелся без жертв.\";",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,15,1+2+4+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,15,4+8+16+32,1+2+4+8+16);",
              "        }",
              "        else if(curEvent==10) {",
              "            newsText=\"Статистические исследования показали, что один из наивысших показателей уровня жизни относительно уровня развития зафиксирован на планете <Name(IdToPlanet(planetColonyId))>. Коренное население - мензолы, проживающие там и по сей день. Несмотря на достаточно малый возраст данной расы, мензолы уже начали контактировать с представителями Коалиции и даже осваивать ближайший космос. Рывок в технологическом и культурном развитии данной цивилизации стал возможен благодаря постройке рейнджером <ShortName(Player())> планетарного центра на данной планете.\";",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+2+4+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+2+4+8+16);",
              "        }",
              "        else if(curEvent==11) {",
              "            newsText=\"Неожиданная находка ожидала исследователей на планете <Name(IdToPlanet(planetColonyId))>. Небольшая команда археологов, работавших совместно с местным населением, обнаружила руины древнего города. Данное событие послужило источником вдохновения для мензолов, которые уже отразили его в своем искусстве.\";",
              "            ChangeGlobalRelationsPlanets(Player(),0,2,10,1+2+4+8+16);",
              "            ChangeGlobalRelationsShips(Player(),0,2,10,4+8+16+32,1+2+4+8+16);",
