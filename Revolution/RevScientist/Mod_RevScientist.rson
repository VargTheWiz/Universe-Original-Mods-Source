          "Code.Type":  "Init",
          "Code":          
          [
              "//vState == 0 - начальный стейт",
              "//vState == 1 - научка взорвана, выводится новость, капсула ждет подбора",
              "//vState == 2 - игрок подобрал капсулу",
              "//vState == 3 - капсула привезена на ПБ",
              "//vState == 4 - квест пройден, сброс фазера в космос",
              "vLastSBName = Name(GroupToShip(SBGroup));",
              "vNewSBName = \"Аннигиляции\";",
              "AddPlanetNews(\"Новости научного мира:<br> <vLastSBName> в системе <Name(Star)> торжественно была переименована в Научную Базу <vNewSBName>! Однако, ближайшие пару дней ученые собираются посвятить синтезу спиртосодержащих веществ, которые впоследствии планируется использовать для протирания сверхточного оборудования, поэтому антидоминаторские исследования приостановлены. По подсчетам наших аналитиков, переименование станции обошлось Межзвездной Коалиции в <GalaxyMoney(2)> cr.\");",
              "SetName(GroupToShip(SBGroup), vNewSBName);",
              "vTimer = CurTurn() + 10;"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  150,
          "Pos.y":  270,
          "Parent":  -1,
          "#":  44,
          "Total.Lines":  9,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if (ShipInStar(Player(), Star)) {",
              "  if (vState == 1) {",
              "    if (!ItemIn(iCapsuleP, Player())) Ether(1, '8E4B1F72-8A0A-441E-9C5F-49AF80B98E36', \"Ааааааа!!! Я два дня просидел в этой проклятой штуковине, и она, похоже, не открывается ни изнутри ни снаружи! Отвези меня... нет, военные меня посадят или даже расстреляют за то что я сделал... На пиратскую базу меня отвези. За это получишь от меня... Черт, ничего ведь нет... Э-ээ... А, получишь пушку крутую. Черт, черт, черт! В этой жестянке обратной связи нет... Короче, рейнджер, надеюсь на твою жадность - помоги мне, а я помогу тебе... Я в капсуле спасательной. На месте, где <clr>Научная База<clrEnd> <vNewSBName>... была...\");",
              "    else vState = 2;",
              "  }",
              "}",
              "if (vState == 1 || vState == 2) {",
              "  if(!ItemExist(iCapsuleP)) AllShipOut();",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  440,
          "Pos.y":  380,
          "Parent":  -1,
          "#":  45,
          "Total.Lines":  14,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if (vState == 3) {",
              "  if (CurTurn() >= vTimer) {",
              "    Dialog(dDoctor, PBGroup);",
              "  }",
              "} else if (vState == 4) {",
              "  i = CreateEquipment(t_Weapon6, 10, GalaxyTechLevel(), PirateClan);",
              "  ModuleToEquipment(FindBonusByNameInCfg('120'), i);",
              "  SetName(i, '<color=255,167,84>' + \"Ручной фазер\" + '</color>');",
              "  AddItemToShip(GroupToShip(PBGroup), i);",
              "  LinkItemToScript(i, iFazer);",
              "  DropScriptItem(CurShip, iFazer);",
              "  ReleaseItemFromScript(iFazer);",
              "  AllShipOut();",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  330,
          "Pos.y":  410,
          "Parent":  -1,
          "#":  46,
          "Total.Lines":  10,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if (!vState) {",
              "  if (CurTurn() >= vTimer) {",
              "    if (Dist(Player(), PlaceSB) > 200) {",
              "      DropScriptItem(CurShip, iCapsuleP);",
              "      AddPlanetNews(\"Срочная новость!<br> Сегодня произошел взрыв в системе <Name(Star)>, взорвалась Научная База <vNewSBName>. Предварительной версией произошедшей трагедии эксперты считают взрыв топливного бака.\");",
              "      vState = 1;",
              "      ShipDestroy(CurShip);",
              "    }",
              "  }",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  240,
