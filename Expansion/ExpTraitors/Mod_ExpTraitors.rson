          "Code":          
          [
              "//Добавление рейнджеров-изменников",
              "bornData = CurTurn() + 30 + Rnd(1, 30);",
              "for(i = 0; i < GalaxyStars(); i = i + 1)",
              "{",
              "    if(2 * GroupCount(Traitors) > ControlledSystems(2)) break;",
              "    if(StarOwner(GalaxyStar(i)) != 2) continue;",
              "    if(StarCustomFaction(GalaxyStar(i)) != '') continue;",
              "    for(j = 0; j < StarPlanets(GalaxyStar(i)); j = j + 1)",
              "    {",
              "        if(2 * GroupCount(Traitors) > ControlledSystems(2)) break;",
              "        dwd0 = StarPlanets(GalaxyStar(i), j);",
              "        if(PlanetOwner(dwd0) == 7)",
              "        {",
              "            dwd1 = BuyRanger(dwd0, 200);",
