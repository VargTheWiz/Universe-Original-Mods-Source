          "Code.Type":  "Turn",
          "Code":          
          [
              "dwd0=ShipStar(Player());",
              "domBon=0;",
              "relation=0;",
              "ExtraBon=0;",
              "",
              "for(i=GalaxyEvents()-1;i>=0;i=i-1)",
              "{",
              "    if(GalaxyEventDate(i)<CurTurn())break;",
              "    //Репутация, снимаемая за рэкет",
              "    if(GalaxyEventType(i)=='PlayerExtortsMoney')relation=relation-8;",
              "    if(GalaxyEventType(i)=='PlayerExtortsGoods')relation=relation-8;",
              "    if(GalaxyEventType(i)=='PlayerKillsShip')",
              "    {",
              "        //Репутация, снимаемая/добавляемая за уничтожение кораблей/станций",
              "        if(GalaxyEventData(i,0)==t_Kling)domBon=domBon+5;",
              "        if(GalaxyEventData(i,0)==t_Ranger)relation=relation-10;",
              "        if(GalaxyEventData(i,0)==t_Transport)relation=relation-10;",
              "        if(GalaxyEventData(i,0)==t_Pirate)relation=relation+10;",
              "        if(GalaxyEventData(i,0)==t_RC)relation=relation-15;",
              "        if(GalaxyEventData(i,0)==t_PB)relation=relation+15;",
              "        if(GalaxyEventData(i,0)==t_WB)relation=relation-15;",
              "        if(GalaxyEventData(i,0)==t_SB)relation=relation-15;",
              "        if(GalaxyEventData(i,0)==t_BK)relation=relation-15;",
              "        if(GalaxyEventData(i,0)==11)relation=relation-15;",
              "        if(GalaxyEventData(i,0)==t_CB)relation=relation+15;",
              "        if(GalaxyEventData(i,0)==13)",
              "        {",
              "            //Нейролинк",
              "            if(GalaxyEventsTextData(i,2)=='KB')relation=relation+15;",
              "            //Центр наёмников",
              "            if(GalaxyEventsTextData(i,2)=='MB')ExtraBon=ExtraBon-15;",
              "            //Ингибитор",
              "            if(GalaxyEventsTextData(i,2)=='Inhibitor')ExtraBon=ExtraBon-80;",
              "        }",
              "        //Штраф репутации за уничтожение военных и флагманов",
              "        if(GalaxyEventData(i,0)==t_Warrior)",
              "        {",
              "            relation=relation-10;",
              "            if(findsubstr(GalaxyEventsTextData(i,1),CT('ShipType.Maloc.WarriorBig'))==0)relation=relation-15;",
              "            if(findsubstr(GalaxyEventsTextData(i,1),CT('ShipType.Peleng.WarriorBig'))==0)relation=relation-15;",
              "            if(findsubstr(GalaxyEventsTextData(i,1),CT('ShipType.People.WarriorBig'))==0)relation=relation-15;",
              "            if(findsubstr(GalaxyEventsTextData(i,1),CT('ShipType.Fei.WarriorBig'))==0)relation=relation-15;",
              "            if(findsubstr(GalaxyEventsTextData(i,1),CT('ShipType.Gaal.WarriorBig'))==0)relation=relation-15;",
              "        }",
              "    }",
              "}",
              "if(relation!=0)",
              "{",
              "    if(StarRuins(dwd0))",
              "    {",
              "        for(i=0;i<StarRuins(dwd0);i=i+1)",
              "        {",
              "            dwd1=StarRuins(dwd0,i);",
              "            if(RelationToRanger(dwd1,Player())<10)continue;",
              "            if(ShipType(dwd1)=='RC' || ShipType(dwd1)=='WB' || ShipType(dwd1)=='SB' || ShipType(dwd1)=='BK' || ShipType(dwd1)=='TC')",
              "            {",
              "                int0=RelationToRanger(dwd1,Player())+relation+ExtraBon+domBon;",
              "                if(int0>100)RelationToRanger(dwd1,Player(),100);",
              "                if(int0<10)RelationToRanger(dwd1,Player(),10);",
              "                if(int0>9 && int0<=100)RelationToRanger(dwd1,Player(),int0);",
              "                if(ShipOwner(Player())==7)",
              "                {",
              "                    if(RelationToRanger(dwd1,Player())>50 && ShipType(dwd1)!='BK')RelationToRanger(dwd1,Player(),50);",
              "                    if(ShipType(dwd1)=='WB')RelationToRanger(dwd1,Player(),10);",
              "                }",
              "            }",
              "            if(ShipType(dwd1)=='PB' || ShipType(dwd1)=='CB')",
              "            {",
              "                int0=RelationToRanger(dwd1,Player())-relation+ExtraBon+domBon;",
              "                if(int0>100)RelationToRanger(dwd1,Player(),100);",
              "                if(int0<10)RelationToRanger(dwd1,Player(),10);",
              "                if(int0>9 && int0<=100)RelationToRanger(dwd1,Player(),int0);",
              "                if(ShipOwner(Player())!=7)",
              "                {",
              "                    if(RelationToRanger(dwd1,Player())>50)RelationToRanger(dwd1,Player(),50);",
              "                }",
              "            }",
              "        }",
              "    }",
              "    relation=0;",
              "    domBon=0;",
              "    ExtraBon=0;",
              "}"          
          ]        
        },
