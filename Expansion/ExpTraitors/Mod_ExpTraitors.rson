              "            if(CurTurn() <= 666)",
              "            {",
              "                if(!(Id(dwd1) % 6))",
              "                {",
              "                    while(ShipPilotRace(dwd1) == People) ShipPilotRace(dwd1, Rnd(Maloc, Gaal));",
              "                }",
              "            }",
              "            ItemSize(ShipItems(dwd1, 0), Rnd(220, 300) + (50 * GalaxyTechLevel()));",
              "            HullHP(dwd1, ItemSize(ShipItems(dwd1, 0)));",
              "            ShipRefit(dwd1);",
              "            ShipType(dwd1, 'RangerPirate');",
              "            ShipJoin(Traitors, dwd1);",
              "            ShipJoinsClan(dwd1);",
