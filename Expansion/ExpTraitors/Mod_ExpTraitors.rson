              "curStar=Id(dwd0);",
              "newStar=curStar;",
              "",
              "//Выставляем начальное отношение баз в родной системе игрока",
              "if(StarRuins(dwd0))",
              "{",
              "    for(i = 0; i < StarRuins(dwd0); i = i + 1)",
              "    {",
              "        dwd1 = StarRuins(dwd0, i);",
              "        if(RelationToRanger(dwd1,Player()) < 10) continue;",
              "        //Коалиционные базы",
              "        if(ShipType(dwd1)=='RC' || ShipType(dwd1)=='SB' || ShipType(dwd1)=='BK' || ShipType(dwd1)=='TC')",
              "        {",
              "            //Глобальное отношение планет людей, фэй и гаальцев",
              "            int0=GlobalRelationsPlanets(Player(),0,4+8+16)+RelationToRanger(dwd1,Player());",
              "            int0=int0/2;",
              "            if(int0>100)RelationToRanger(dwd1,Player(),100);",
              "            //Не даём начальному отношению опуститься слишком низко, иначе игрока может послать даже стартовый ЦР",
              "            else if(int0<30)RelationToRanger(dwd1,Player(),30);",
              "            else if(int0>29 && int0<=100)RelationToRanger(dwd1,Player(),int0);",
              "        }",
              "        //Военная база",
              "        if(ShipType(dwd1)=='WB')",
              "        {",
              "            //Глобальное отношение планет людей, фэй и гаальцев",
              "            int0=GlobalRelationsPlanets(Player(),0,4+8+16)+RelationToRanger(dwd1,Player());",
              "            int0=int0/2;",
              "            if(int0>75)RelationToRanger(dwd1,Player(),75);",
              "            else if(int0<30)RelationToRanger(dwd1,Player(),30);",
              "            else if(int0>29 && int0<=75)RelationToRanger(dwd1,Player(),int0);",
